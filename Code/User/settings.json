{
  "workbench.colorTheme": "GitHub Dark",
  "editor.experimental.asyncTokenization": true,
  "editor.stickyScroll.enabled": true,
  "files.insertFinalNewline": true,
  "workbench.startupEditor": "none",
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "workbench.preferredHighContrastColorTheme": "GitHub Dark High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "GitHub Light High Contrast",
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.fontAliasing": "auto",
  "window.autoDetectColorScheme": true,
  "search.smartCase": true,
  "terminal.integrated.shellIntegration.suggestEnabled": true,
  "remote.downloadExtensionsLocally": true,
  "telemetry.telemetryLevel": "off",
  "git.autofetch": true,
  "git.autoStash": true,
  "git.enableSmartCommit": true,
  "git.followTagsWhenSync": true,
  "git.fetchOnPull": true,
  "git.rebaseWhenSync": true,
  "github.codespaces.showPerformanceExplorer": true,
  "remoteHub.gitProtocol": "ssh",
  "editor.fontFamily": "MonaspiceKr NF",
  "editor.rulers": [
    100
  ],
  "workbench.colorCustomizations": {
    "terminal.ansiBlack": "#8f8e92",
    "terminal.ansiRed": "#ff3b2f",
    "terminal.ansiGreen": "#2acd41",
    "terminal.ansiYellow": "#fecc02",
    "terminal.ansiBlue": "#007bff",
    "terminal.ansiMagenta": "#b052de",
    "terminal.ansiCyan": "#54bef0",
    "terminal.ansiWhite": "#fffeff",
    "terminal.ansiBrightBlack": "#8f8e92",
    "terminal.ansiBrightRed": "#ff3b2f",
    "terminal.ansiBrightGreen": "#2acd41",
    "terminal.ansiBrightYellow": "#fecc02",
    "terminal.ansiBrightBlue": "#007bff",
    "terminal.ansiBrightMagenta": "#b052de",
    "terminal.ansiBrightCyan": "#54bef0",
    "terminal.ansiBrightWhite": "#fffeff"
  },
  "files.autoSave": "afterDelay",
  "editor.linkedEditing": true,
  "editor.smoothScrolling": true,
  "editor.find.globalFindClipboard": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "files.readonlyFromPermissions": true,
  "workbench.enableExperiments": false,
  "workbench.list.smoothScrolling": true,
  "workbench.tree.enableStickyScroll": true,
}
